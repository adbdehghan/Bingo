  pod 'BarcodeScanner'
  pod 'TCPickerView'
  pod 'JHSpinner'  
  pod 'Fabric'
  pod 'Crashlytics'
  pod 'Firebase/Core'
  pod 'Firebase/RemoteConfig'
  
  target 'BingoTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'BingoUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
